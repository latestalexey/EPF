//Работа со свойствами

Функция ПолучитьОбъектПоЗначениюСвойства(НаименованиеСвойства, ЗначениеОбъекта, НазначениеСвойства)	
	
	Запрос = Новый Запрос("ВЫБРАТЬ
	                      |	ЗначенияСвойствОбъектов.Объект
	                      |ИЗ
	                      |	РегистрСведений.ЗначенияСвойствОбъектов КАК ЗначенияСвойствОбъектов
	                      |ГДЕ
	                      |	ЗначенияСвойствОбъектов.Значение = &ЗначениеОбъекта
	                      |	И ЗначенияСвойствОбъектов.Свойство.Наименование = &НаименованиеСвойства
	                      |	И ЗначенияСвойствОбъектов.Свойство.НазначениеСвойства = &НазначениеСвойства");
	Запрос.УстановитьПараметр("ЗначениеОбъекта", ЗначениеОбъекта);
	Запрос.УстановитьПараметр("НаименованиеСвойства", НаименованиеСвойства);
	Запрос.УстановитьПараметр("НазначениеСвойства", НазначениеСвойства);
	Выборка = Запрос.Выполнить().Выбрать();
	Если Выборка.Следующий() Тогда
		Возврат Выборка.Объект.Ссылка;
	Иначе
		Возврат Неопределено;
	КонецЕсли;
	
КонецФункции


Функция ПолучитьЗначениеСвойства(Объект, ИмяСвойства, НазначениеСвойства)	
	
	Запрос = Новый Запрос("ВЫБРАТЬ
	                      |	ЗначенияСвойствОбъектов.Значение
	                      |ИЗ
	                      |	РегистрСведений.ЗначенияСвойствОбъектов КАК ЗначенияСвойствОбъектов
	                      |ГДЕ
	                      |	ЗначенияСвойствОбъектов.Объект = &Объект
	                      |	И ЗначенияСвойствОбъектов.Свойство.Наименование = &НаименованиеСвойства
	                      |	И ЗначенияСвойствОбъектов.Свойство.НазначениеСвойства = &НазначениеСвойства");
	Запрос.УстановитьПараметр("Объект", Объект);
	Запрос.УстановитьПараметр("НаименованиеСвойства", ИмяСвойства);
	Запрос.УстановитьПараметр("НазначениеСвойства", НазначениеСвойства);
	Выборка = Запрос.Выполнить().Выбрать();
	Если Выборка.Следующий() Тогда
		Возврат Выборка.Значение;
	Иначе
		Возврат Неопределено;
	КонецЕсли;
	
КонецФункции



